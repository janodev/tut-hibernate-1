<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%-5p [%t] %F:%L - %m%n" />
		</Console>
	</appenders>
	<loggers>
		<logger name="org.hibernate" level="warn"/>
		<root level="trace">
			<appender-ref ref="STDOUT" />
		</root>
	</loggers>
</configuration>